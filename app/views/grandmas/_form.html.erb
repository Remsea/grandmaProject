<%= simple_form_for(@grandma) do |f| %>
  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.input :price %>
  <%= f.input :address %>
  <%= f.input :age %>
  <%= f.association :competence, collection: @competences %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.button :submit, class: 'gmp-btn gmp-btn:hover' %>
<% end %>
